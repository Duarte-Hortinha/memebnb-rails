
<div class="card mb-3">
  <div class="row g-0">
    <% @my_memes.each  do |meme| %>
      <div class="col-md-3">
       <%= image_tag "code.png", height: 200, width: "100%" %>
      </div>
      <div class="col-md-3">
        <div class="card-body">
          <h5 class="card-title"><%= meme.title %></h5>
          <p class="card-text">$<%= meme.price %></p>
          <P>Belongs to <%= meme.user.email.split('@')[0] %></p>
          <p class="card-text"><small class="text-body-secondary">$<%= meme.price %></small></p>
        </div>
      </div>
      <div class="col-md-6">
        
        <%= link_to "update", edit_memes_path(meme.id), class:"btn btn-info" %>
        <%= link_to "Delete", delete_path(meme.id), class:"btn btn-danger" %>
      </div>
    <% end %>
  </div>
</div>
